<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.runit">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application

        android:allowBackup="true"
        android:icon="@drawable/logo_run_it"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_run_it"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
                  android:theme="@style/AppTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activityinstalleracc"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".Activitycreateacc"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".Activitydatapreferenceacc"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".Activitylogon"
            android:theme="@style/AppTheme" />


    </application>

</manifest>