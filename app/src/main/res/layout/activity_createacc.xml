<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:screenOrientation="portrait"
    tools:context="com.runnerup.runit.Activitycreateacc">
    <!--  style="@style/Widget.AppCompat.EditText"-->

    <Button
        android:id="@+id/organisationbuttin"
        android:layout_width="295dp"
        android:layout_height="50dp"
        android:background="@drawable/mywhitebutton"
        android:fontFamily="sans-serif-condensed"
        android:text="ORGANISATION"
        android:textAlignment="center"

        android:textAllCaps="false"
        android:textColor="#E91E63"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.553" />

    <Button
        android:id="@+id/teambuttin"
        android:layout_width="295dp"
        android:layout_height="50dp"
        android:background="@drawable/mywhitebutton"
        android:fontFamily="sans-serif-condensed"
        android:text="TEAM"
        android:textAlignment="center"

        android:textAllCaps="false"
        android:textColor="#E91E63"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.439" />

    <Button
        android:id="@+id/individualbuttin"
        android:layout_width="295dp"
        android:layout_height="50dp"
        android:background="@drawable/mywhitebutton"
        android:fontFamily="sans-serif-condensed"
        android:text="INDIVIDUAL"
        android:textAlignment="center"

        android:textAllCaps="false"
        android:textColor="#E91E63"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.328" />

    <TextView
        android:id="@+id/textViewnewaccountlogon"
        android:layout_width="340dp"
        android:layout_height="37dp"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-condensed"
        android:textAlignment="textStart"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.408"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.871" />

    <Button
        android:id="@+id/dataprofilebutton"
        android:layout_width="295dp"
        android:layout_height="50dp"
        android:background="@drawable/myrndredkbutton"
        android:fontFamily="sans-serif-condensed"
        android:text="CREATE"
        android:textAlignment="center"

        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.792" />

    <ProgressBar
        android:id="@+id/progressBarcreate"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.91" />


    <EditText
        android:id="@+id/editTextlnamecreate"
        android:layout_width="183dp"
        android:layout_height="39dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="Last Name ?.."
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#0040E0"
        android:textColorHint="#E91E63"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.929"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.164" />

    <EditText
        android:id="@+id/editTextfnamecreate"
        android:layout_width="169dp"
        android:layout_height="39dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="First Name ?.."
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#0040E0"
        android:textColorHint="#E91E63"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.119"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.164" />

    <EditText
        android:id="@+id/editTextnicknamecreate"
        android:layout_width="140dp"
        android:layout_height="44dp"
        android:ems="10"
        android:fontFamily="sans-serif-smallcaps"
        android:hint="Nick name.. ?"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#0040E0"
        android:textColorHint="#E91E63"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.874"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.229" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="291dp"
        android:layout_height="31dp"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-condensed"
        android:text="Create a Password below .."
        android:textAlignment="textStart"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.583"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.634" />

    <TextView
        android:id="@+id/textViewnewlogonID"
        android:layout_width="340dp"
        android:layout_height="37dp"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-condensed"
        android:textAlignment="textStart"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.408"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.951" />

    <TextView
        android:id="@+id/textView3create"
        android:layout_width="193dp"
        android:layout_height="29dp"
        android:background="#FFFFFF"
        android:fontFamily="sans-serif-condensed"
        android:text="Hello, I am .."
        android:textAlignment="center"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.246" />

    <Button
        android:id="@+id/dataprefbuttonin"
        android:layout_width="150dp"
        android:layout_height="44dp"
        android:background="@drawable/myrndredkbutton"
        android:fontFamily="sans-serif-smallcaps"
        android:text="DATA PREFERFENCES"
        android:textAlignment="center"

        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        android:typeface="monospace"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.991" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="270dp"
        android:layout_height="37dp"
        android:ems="10"
        android:hint="minimum 7 alpha-numeric characters"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColor="#0040E0"
        android:textColorHint="#ED0847"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.695" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="355dp"
        android:layout_height="73dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.392"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039"
        app:srcCompat="@drawable/bannernew" />


</androidx.constraintlayout.widget.ConstraintLayout>